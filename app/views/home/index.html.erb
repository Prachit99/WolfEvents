<p id=”notice”><%= notice %></p>

<h1>Wolf Events</h1>
<p>Purchase event tickets at the snap of your finger</p>
<% if current_admin %>
  Hello <%= current_admin.name %><br>
  <%= link_to 'View your profile', admin_path(current_admin) %><br>
  <%= link_to 'View all events', events_path %><br>
  <%= link_to 'View all attendees', attendees_path %><br>
  <%= link_to 'View all event tickets', event_tickets_path %><br>
  <%= link_to 'View all rooms', rooms_path %><br>
  <%= link_to 'View all reviews', reviews_path %><br>
  <%= link_to "Logout", logout_path %>
<% elsif current_user %>
    Hello <%= current_user.name %><br>
    <%= link_to 'View your profile', attendee_path(current_user) %><br>
    <%= link_to 'View all event', events_path %><br>
    <%= link_to 'View upcoming event', upcoming_events_events_path %><br>
    <%= link_to 'View occured events', past_events_events_path %><br>
    <%= link_to 'View event history', event_tickets_path %><br>
    <%= link_to "Logout", logout_path %>
<% else %>>
    <%= link_to "Signup", signup_path %> or
    <%= link_to "Login", login_path %>
<% end %>
